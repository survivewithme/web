{
  "version": 2,
  "builds": [
    {
      "src": "./build/index.html",
      "use": "@now/static"
    },
    {
      "src": "./build/main.js",
      "use": "@now/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/build/index.html"
    },
    {
      "src": "/offer/.+",
      "dest": "/build/index.html"
    },
    {
      "src": "/main.js",
      "dest": "/build/main.js"
    }
  ]
}
